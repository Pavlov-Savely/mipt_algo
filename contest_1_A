#include <iostream>
#include <vector>
#include <string>

using namespace std; 

vector<int> PrefixFunction(string s) {
  int n = s.length();  
  vector<int> pi(n);
  for (int i = 1; i < n; ++i) {
    int j = pi[i - 1];
    while (j > 0 && s[i] != s[j]) {
      j = pi[j - 1];
    }
    if (s[i] == s[j]) {
      j++;
    }
    pi[i] = j;
  }
  return pi;
}

int main() {
  string str;
  string obr;
  
  cin >> str;
  cin >> obr;

  string s = obr + '#' + str;

  vector<int> p = PrefixFunction(s);

  for (int i = obr.length() + 1; i < p.size(); ++i) { 
    if (p[i] == obr.length()) { 
      cout << i - 2 * obr.length() << endl; 
    }
  }

  return 0;
}
